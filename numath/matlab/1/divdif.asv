function c = divdif(xdata, ydata)
    if length(xdata) ~= length(ydata)
        c = 'failed';
        return;
    end
    
    if length(xdata) == 1
        c = ydata(1);
        return;
    end
    
    c = divdif(xdata(2:length(xdata)),   ydata(2:length(xdata))) ...
      - divdif(xdata(1:length(xdata)-1), ydata(1:length(xdata)-1)) ...
end
